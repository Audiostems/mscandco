HOST=0.0.0.0
PORT=1337
APP_KEYS="prod-key-1,prod-key-2"
API_TOKEN_SALT=prod-api-token-salt
ADMIN_JWT_SECRET=prod-admin-jwt-secret
TRANSFER_TOKEN_SALT=prod-transfer-token-salt
JWT_SECRET=prod-jwt-secret

# AWS RDS Database Configuration
DATABASE_CLIENT=postgres
DATABASE_HOST=your-rds-endpoint.region.rds.amazonaws.com
DATABASE_PORT=5432
DATABASE_NAME=audiostems
DATABASE_USERNAME=audiostems
DATABASE_PASSWORD=your-secure-database-password
DATABASE_SSL=true

# Environment
NODE_ENV=production

# Client URL for Stripe
CLIENT_URL=https://your-domain.com

# Stripe Configuration (use live keys for production)
STRIPE_SECRET=sk_live_your_stripe_live_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret_here

# AWS S3 Configuration
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_ACCESS_SECRET_KEY=your-aws-secret-key
AWS_BUCKET=your-s3-bucket-name
AWS_REGION=us-east-1

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Facebook OAuth
FACEBOOK_CLIENT_ID=your-facebook-client-id
FACEBOOK_CLIENT_SECRET=your-facebook-client-secret

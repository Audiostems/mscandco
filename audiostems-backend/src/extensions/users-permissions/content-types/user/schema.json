{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string"
    },
    "stripeCustomerId": {
      "type": "string"
    },
    "productId": {
      "type": "string"
    },
    "planId": {
      "type": "string"
    },
    "planActive": {
      "type": "boolean"
    },
    "credit": {
      "type": "integer"
    },
    "subscriptionId": {
      "type": "string"
    },
    "downloadHistories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::download-history.download-history",
      "mappedBy": "user"
    },
    "emailVerified": {
      "type": "boolean",
      "default": false
    },
    "mobileVerified": {
      "type": "boolean",
      "default": false
    },
    "profileComplete": {
      "type": "boolean",
      "default": false
    },
    "emailVerificationCode": {
      "type": "string",
      "private": true
    },
    "emailVerificationExpires": {
      "type": "datetime"
    },
    "mobileNumber": {
      "type": "string"
    },
    "mobileVerificationCode": {
      "type": "string",
      "private": true
    },
    "mobileVerificationExpires": {
      "type": "datetime"
    },
    "recoveryCodes": {
      "type": "json",
      "private": true
    },
    "company": {
      "type": "string"
    },
    "jobTitle": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "bio": {
      "type": "text"
    },
    "profileImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": ["images"]
    },
    "userRole": {
      "type": "enumeration",
      "enum": [
        "artist",
        "distribution_partner",
        "company_admin",
        "super_admin"
      ],
      "default": "artist"
    },
    "bankDetails": {
      "type": "component",
      "component": "shared.bank-details",
      "private": true
    },
    "earnings": {
      "type": "component",
      "component": "shared.earnings",
      "private": true
    },
    "monthlyStatements": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::monthly-statement.monthly-statement",
      "mappedBy": "user"
    }
  }
}
